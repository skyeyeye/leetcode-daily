class Solution {
public:
    bool isValid(string s) {
        stack<int> st;
        for(int i = 0; i < s.size(); i++){
            if(st.empty()){
                if(s[i] == ')' || s[i] == '}' || s[i] == ']'){
                    return false;
                }
                else{
                    st.push(s[i]);
                }

            }
            else{
                if ( (st.top() == '(' && s[i] == ')' ) || (s[i] == '}' && st.top() == '{' ) || (s[i] == ']' && st.top() == '[' )  ) {
                    st.pop();
                }
                else{
                    st.push(s[i]);
                }
            }
        }
        if(st.empty()) return true;
        else return false;
        // stack<char>st;
        // for(int i = 0; i < s.size(); i++){
            
        //     if(s[i] == '(' || s[i] == '{'|| s[i] == '['){
        //         st.push(s[i]);
        //     }
        //     else{
        //        if(st.empty()) return 0;
        //        else if(s[i] == ')'){
        //         if(st.top() != '(') return 0;
        //          else{
        //              st.pop();
        //         }
        //        }
        //         else if(s[i] == '}'){
        //             if(st.top() != '{') return 0;
        //             else{
        //                 st.pop();
        //             }
        //         }
        //         else if(s[i] == ']'){
        //             if(st.top() != '[') return 0;
        //             else{
        //                 st.pop();
        //             }
        //         }
               
        //     }
        // }
        // return st.empty();
    }
};
